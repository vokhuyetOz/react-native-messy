{
  "name": "react-native-messy",
  "version": "0.1.0",
  "description": "chat UI",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-messy.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/vokhuyetOz/react-native-messy",
  "author": "Vokhuyet <vokhuyet.ict@gmail.com> (https://github.com/vokhuyetOz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vokhuyetOz/react-native-messy/issues"
  },
  "homepage": "https://github.com/vokhuyetOz/react-native-messy#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^18.0.12",
    "@types/react-native": "^0.67.8",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react-native-builder-bob": "^0.18.0",
    "react-native-lightbox-v2": "^0.9.0",
    "react-native-reanimated": "^2.8.0",
    "react-native-ui-lib": "^6.17.1",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3",
    "@react-native-community/cameraroll":"^4.1.2",
    "react-native-image-picker":"^4.8.4",
    "fbjs":"^3.0.4",
    "react-native-audio-recorder-player":"^3.5.1"
  },
  "dependencies": {
    "@stream-io/flat-list-mvcp": "^0.10.2",
    "dayjs": "1.11.2",
    "react-native-bidirectional-infinite-scroll": "0.3.3",
    "react-native-keyboard-controller": "^1.0.0-beta.0",
    "react-native-lightbox-v2": "^0.9.0",
    "react-native-parsed-text": "^0.0.22"
  },
  "peerDependencies": {
    "@stream-io/flat-list-mvcp": "*",
    "dayjs": "*",
    "react": "*",
    "react-native": "*",
    "react-native-bidirectional-infinite-scroll": "*",
    "react-native-keyboard-controller": "*",
    "react-native-lightbox-v2": "*",
    "react-native-parsed-text": "*",
    "react-native-reanimated": "*",
    "@react-native-community/cameraroll":"*",
    "react-native-image-picker":"*",
    "fbjs":"*",
    "react-native-audio-recorder-player":"*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
